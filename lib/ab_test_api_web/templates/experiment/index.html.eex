<h1>Listing Experiments</h1>

<table>
  <thead>
    <tr>
      <th>Key</th>

      <th></th>
    </tr>
  </thead>
  <tbody>
<%= for experiment <- @experiments do %>
    <tr>
      <td><%= experiment.key %></td>

      <td>
        <%= link "Show", to: Routes.experiment_path(@conn, :show, experiment) %>
        <%= link "Delete", to: Routes.experiment_path(@conn, :delete, experiment), method: :delete, data: [confirm: "Are you sure?"] %>
      </td>
    </tr>
<% end %>
  </tbody>
</table>

<span><%= link "New Experiment", to: Routes.experiment_path(@conn, :new) %></span>
