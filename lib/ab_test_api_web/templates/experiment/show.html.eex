<h1>Show Experiment</h1>

<ul>

  <li>
    <strong>Key:</strong>
    <%= @experiment.key %>
  </li>

  <li>
    <strong>Options:</strong>
    <ul>
      <%= for option <- @experiment.options do %>
          <li>
            <strong>value:</strong>
            <%= option.value %>
            <strong>percentage:</strong>
            <%= option.percentage %>
          </li>
      <% end %>
    </ul>
  </li>

</ul>

<span><%= link "Back", to: Routes.experiment_path(@conn, :index) %></span>
